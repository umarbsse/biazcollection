const Price = ( { fullPrice, discountedPrice, currency, hidePriceFraction } ) => {
	const finalPrice = discountedPrice > 0 ? discountedPrice : fullPrice;

	const { symbol, integer, fraction } = getCurrencyObject( finalPrice, currency );
