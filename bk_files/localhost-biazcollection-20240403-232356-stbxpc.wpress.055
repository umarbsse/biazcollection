    }

    /**
     * @return array|null
     */
    public function getDescriptor()
    {
        return $this->descriptor;
    }

    /**
     * @param mixed|Message $message
     * @return Call
     */
    public function withMessage($message)
    {
        // @phpstan-ignore-next-line
        return new static(
            $this->method,
            $this->decodeType,
            $message,
            $this->descriptor,
            $this->callType
        );
