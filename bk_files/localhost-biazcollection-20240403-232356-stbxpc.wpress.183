				text-decoration: none;
				box-shadow: none;

				&::before {
					display: block;
					position: absolute;
					top: 0;
					left: 0;
					width: 1em;
					height: 1em;
					line-height: 1;
					font-family: WooCommerce;
					content: "\e021";
					text-indent: 0;
				}

				&:hover {
					~ a::before {
						content: "\e021";
					}
				}
			}

			&:hover {
				a {
					&::before {
						content: "\e020";
					}
				}
			}

			&.selected {
