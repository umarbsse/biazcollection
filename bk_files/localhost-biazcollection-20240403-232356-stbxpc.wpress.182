	display: flex;
	flex-flow: column;
}

.wc-logs-search-fieldset {
	display: flex;
	gap: 0.5em;
	justify-content: flex-end;
}

.wc-logs-search-notice {
	font-size: 0.9em;
	line-height: 2;
	text-align: right;
	visibility: hidden;
	height: 0;

	.wc-logs-search:focus-within & {
		visibility: visible;
		height: auto;
	}
}

.wc-logs-entries {
	background: #f6f7f7;
	border: 1px solid #c3c4c7;
	font-family: Consolas, Monaco, monospace;
	word-wrap: break-word;
	line-height: 2.3;
	counter-reset: line;

	.line {
		display: block;
		width: 100%;
		position: relative;
		white-space: pre-wrap;

		&:before {
			counter-increment: line;
			content: counter(line);
			display: block;
			float: left;
			padding: 0 1em 0 0.5em;
			width: 3em;
			text-align: right;
		}

		&:target {
			box-shadow: 0 0.16em 0.16em -0.16em #c3c4c7, 0 -0.16em 0.16em -0.16em #c3c4c7;
			z-index: 1;
			scroll-margin-top: 50vh;

			.line-content {
				background: #fff8c5;
			}

			.log-level:before {
